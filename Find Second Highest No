package com;

public class Demo2 {

	public static void main(String[] args) {
		
		int arr[]= {3,54,6,3,67};
		int size=arr.length;
		int temp;
		
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				if(arr[i]>arr[j])
				{
					temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
					}
				}
			System.out.println(arr[i]);
			}
		System.out.println("2nd Highest NO");
		System.out.println(arr[size-2]);
		}
		}
	
